  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/remix-e2e/src",
  "tags": [],
  "implicitDependencies": ["remix"],
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
