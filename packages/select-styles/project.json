{
  "name": "select-styles",
  "implicitDependencies": ["theme"],
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/select-styles/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/packages/{projectName}"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    }
  }
}
