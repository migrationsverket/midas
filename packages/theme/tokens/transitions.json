{
  "transition": {
    "duration": {
      "slow": {
        "$value": "500ms",
        "$type": "duration"
      },
      "normal": {
        "$value": "300ms",
        "$type": "duration"
      },
      "fast": {
        "$value": "250ms",
        "$type": "duration"
      }
    },
    "timing": {
      "easeOut": {
        "$value": [0, 0, 0.58, 1],
        "$type": "cubicBezier"
      },
      "easeIn": {
        "$value": [0.42, 0, 1, 1],
        "$type": "cubicBezier"
      }
    },
    "panel": {
      "collapse": {
        "$value": {
          "delay": 0,
          "duration": "{transition.duration.normal}",
          "timingFunction": "{transition.timing.easeOut}"
        },
        "$type": "transition"
      },
      "expand": {
        "$value": {
          "delay": 0,
          "duration": "{transition.duration.normal}",
          "timingFunction": "{transition.timing.easeIn}"
        },
        "$type": "transition"
      }
    }
  }
}
