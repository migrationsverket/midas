{
  "repository": {
    "type": "git",
    "url": "git+https://github.com/migrationsverket/midas.git",
    "directory": "packages/theme"
  },
  "name": "@midas-ds/theme",
  "author": {
    "name": "Migrationsverket"
  },
  "publishConfig": {
    "access": "public"
  },
  "description": "Midas Theme",
  "homepage": "https://designsystem.migrationsverket.se/",
  "license": "CC0-1.0",
  "version": "3.7.1",
  "main": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "default": "./index.mjs"
    },
    "./variables.css": {
      "import": "./variables.css",
      "default": "./variables.css"
    },
    "./tailwind-theme.css": {
      "import": "./tailwind-theme.css",
      "default": "./tailwind-theme.css"
    },
    "./fonts.css": {
      "import": "./fonts.css",
      "default": "./fonts.css"
    },
    "./color-scheme.css": {
      "import": "./color-scheme.css",
      "default": "./color-scheme.css"
